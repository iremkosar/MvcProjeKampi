@{
    ViewBag.Title = "SweetAlert";
    Layout = "~/Views/Shared/_AdminLayout.cshtml";
}
<br />

<script src="~/Scripts/jquery-3.7.0.js"></script>
<script src="https://cdn.jsdelivr.net/npm/sweetalert2@11"></script>

<button class="btn btn-primary" id="btn1">Button1</button>
<button class="btn btn-warning" id="btn2">Button2</button>
<button class="btn btn-danger" id="btn3">Button3</button>

<script>
    $(document).ready(function () {
        $('#btn1').click(function () {
            Swal.fire("Merhaba Mvc Proje Kampı");
        });
    });
</script>
<script>
    $(document).ready(function () {
        $('#btn2').click(function () {
            Swal.fire({
                title: "Mvc Proje Kampı!",
                text: "...Ders96!",             
            });
        });
    });
</script>
<script>
    $(document).ready(function () {
        $('#btn3').click(function () {
            Swal.fire({
                title: "Tebrikler!",
                text: "Başarılı bir şekilde eklendi!",
                icon: "success"
            });
        });
    });
</script>

